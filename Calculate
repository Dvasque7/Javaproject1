package com.diegosoft.program;

public class Calculate {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
    int product = 1;
    
    for (int number = 1; number <= 15; number +=2) {
    	product *= number;
    }
    
    System.out.printf("Total product of the odd integers from 1 to 15 is %,d%n", product);
	}

}
